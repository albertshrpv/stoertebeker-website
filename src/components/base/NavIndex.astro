<style>
    /* Nav background transition */
    #mainNav {
        background-color: transparent;
        height: 100px;
        padding-top: 40px; /* Extra space at top when transparent */
        transition:
            background-color 0.3s ease-in-out,
            height 0.3s ease-in-out,
            padding-top 0.3s ease-in-out;
    }

    @media (min-width: 1024px) {
        #mainNav {
            height: 140px;
            padding-top: 80px; /* Extra space at top when transparent on large screens */
        }
    }

    #mainNav.nav-scrolled {
        background-color: white;
        height: 140px;
        padding-top: 0;
    }

    /* Nav links - white when transparent, black when scrolled */
    .nav-link {
        color: white;
        transition: color 0.3s ease-in-out;
    }

    #mainNav.nav-scrolled .nav-link {
        color: #19263d; /* text-darkBlue */
    }

    /* Nav link borders - white when transparent, black when scrolled */
    .nav-link-border {
        border-color: white;
    }

    #mainNav.nav-scrolled .nav-link-border {
        border-color: black;
    }

    /* Burger menu lines - white when transparent, black when scrolled */
    .burger-line {
        background-color: white;
    }

    #mainNav.nav-scrolled .burger-line {
        background-color: #171717; /* bg-neutral-900 */
    }

    /* Logo SVG - white when transparent, black when scrolled */
    /* Main Logo Sizing */
    .main-logo {
        width: 160px;
        transition: width 0.3s ease-in-out;
    }

    @media (min-width: 1024px) {
        .main-logo {
            width: 200px;
        }
    }

    #mainNav.nav-scrolled .main-logo {
        width: 120px;
    }

    @media (min-width: 1024px) {
        #mainNav.nav-scrolled .main-logo {
            width: 148px;
        }
    }

    /* Override any existing fill colors */
    #mainNav svg path,
    #mainNav svg g path,
    #mainNav svg .st0 path {
        fill: white !important;
        transition: fill 0.3s ease-in-out;
    }

    #mainNav.nav-scrolled svg path,
    #mainNav.nav-scrolled svg g path,
    #mainNav.nav-scrolled svg .st0 path {
        fill: black !important;
    }

    /* Keep the tickets SVG always white */
    #mainNav.nav-scrolled svg.tickets-svg path {
        fill: white !important;
    }

    /* Border SVG visibility on scroll */
    .tickets-border-svg {
        opacity: 0;
        transition: opacity 0.3s ease-in-out;
    }

    #mainNav.nav-scrolled .tickets-border-svg {
        opacity: 1;
    }

    /* Also handle stroke if present */
    #mainNav svg path[stroke],
    #mainNav svg g path[stroke] {
        stroke: white;
        transition: stroke 0.3s ease-in-out;
    }

    #mainNav.nav-scrolled svg path[stroke],
    #mainNav.nav-scrolled svg g path[stroke] {
        stroke: black;
    }

    /* Custom dotted border pattern: 6px line, 6px space */
    .border-dotted-custom {
        position: relative;
        border-left: none;
    }
    
    .border-dotted-custom::before {
        content: '';
        position: absolute;
        left: 0;
        top: 0;
        bottom: 0;
        width: 1px;
        background-image: repeating-linear-gradient(
            to bottom,
            black 0px,
            black 4px,
            transparent 4px,
            transparent 8px
        );
    }

    .concave-corners-nav {
        mask-image: 
            radial-gradient(circle 20px at 0 0, transparent 20px, black 21px),
            radial-gradient(circle 20px at 100% 0, transparent 20px, black 21px),
            radial-gradient(circle 20px at 0 100%, transparent 20px, black 21px),
            radial-gradient(circle 20px at 100% 100%, transparent 20px, black 21px);
        mask-position: top left, top right, bottom left, bottom right;
        mask-size: 51% 51%;
        mask-repeat: no-repeat;
        -webkit-mask-image: 
            radial-gradient(circle 20px at 0 0, transparent 20px, black 21px),
            radial-gradient(circle 20px at 100% 0, transparent 20px, black 21px),
            radial-gradient(circle 20px at 0 100%, transparent 20px, black 21px),
            radial-gradient(circle 20px at 100% 100%, transparent 20px, black 21px);
        -webkit-mask-position: top left, top right, bottom left, bottom right;
        -webkit-mask-size: 51% 51%;
        -webkit-mask-repeat: no-repeat;
    }
    
    #megaMenuOverlay {
        transition: opacity 0.3s ease-in-out;
        z-index: 105; /* Between nav background and buttons */
    }
</style>

<script>
    (function () {
        const megaMenuToggle = document.getElementById("megaMenuToggle");
        const mobileMenu = document.getElementById("mobileMenu");
        const megaMenu = document.getElementById("megaMenu");
        const megaMenuOverlay = document.getElementById("megaMenuOverlay");
        const nav = document.getElementById("mainNav");
        
        let isMobileMenuOpen = false;
        let isMegaMenuOpen = false;

        // Scroll detection for nav background
        function handleScroll() {
            if (!nav) return;
            const scrollY = window.scrollY;
            const scrollThreshold = 50;

            if (scrollY > scrollThreshold) {
                nav.classList.add("nav-scrolled");
                // Update mobile menu position when scrolled
                if (mobileMenu) {
                    mobileMenu.style.top = "200px";
                    mobileMenu.style.height = "calc(100vh - 200px)";
                }
            } else {
                nav.classList.remove("nav-scrolled");
                // Update mobile menu position when transparent
                if (mobileMenu) {
                    mobileMenu.style.top = "140px";
                    mobileMenu.style.height = "calc(100vh - 140px)";
                }
            }
        }

        // Initial check
        handleScroll();
        window.addEventListener("scroll", handleScroll);

        // Helper to update burger icon
        function updateBurgerIcon(isOpen: boolean) {
            if (!megaMenuToggle) return;
            const bars = megaMenuToggle.querySelectorAll("span");
            if(bars.length >= 3) {
                if (isOpen) {
                    bars[0].style.transform = "translateY(8px) rotate(45deg)";
                    bars[1].style.opacity = "0";
                    bars[2].style.transform = "translateY(-8px) rotate(-45deg)";
                } else {
                    bars[0].style.transform = "";
                    bars[1].style.opacity = "1";
                    bars[2].style.transform = "";
                }
            }
        }

        function closeMobileMenu() {
            if (!mobileMenu || !isMobileMenuOpen) return;

            isMobileMenuOpen = false;
            mobileMenu.classList.add("translate-x-full");
            
            // Only reset icon if mega menu is not open either (they shouldn't be open together anyway)
            if (!isMegaMenuOpen) {
                updateBurgerIcon(false);
            }

            const body = document.body;
            const top = body.style.top;
            body.style.position = "";
            body.style.top = "";
            body.style.width = "";
            if (top) {
                const y = parseInt(top || "0", 10) * -1;
                window.scrollTo(0, y);
            }
        }

        function toggleMobileMenu() {
            if (!mobileMenu) return;
            
            // Ensure mega menu is closed
            if (isMegaMenuOpen) closeMegaMenu();

            isMobileMenuOpen = !isMobileMenuOpen;
            mobileMenu.classList.toggle("translate-x-full");

            updateBurgerIcon(isMobileMenuOpen);

            // Prevent background scroll when the mobile menu is open
            const body = document.body;
            if (isMobileMenuOpen) {
                const scrollY = window.scrollY;
                body.style.position = "fixed";
                body.style.top = `-${scrollY}px`;
                body.style.width = "100%";
            } else {
                const top = body.style.top;
                body.style.position = "";
                body.style.top = "";
                body.style.width = "";
                if (top) {
                    const y = parseInt(top || "0", 10) * -1;
                    window.scrollTo(0, y);
                }
            }
        }

        function closeMegaMenu() {
            if (!megaMenu || !megaMenuOverlay) return;
            
            isMegaMenuOpen = false;
            megaMenu.classList.remove("opacity-100");
            megaMenuOverlay.classList.remove("opacity-100");
            megaMenu.classList.add("opacity-0");
            megaMenuOverlay.classList.add("opacity-0");
            
            if (!isMobileMenuOpen) {
                updateBurgerIcon(false);
            }
            
            // Enable scroll
            document.body.style.paddingRight = "";
            document.body.style.overflow = "";
            if (nav) {
                nav.style.paddingRight = "";
            }
            if (megaMenu) {
                megaMenu.style.width = '';
                megaMenu.style.right = '';
            }

            setTimeout(() => {
                if (!isMegaMenuOpen) {
                    megaMenu.classList.add("hidden");
                    megaMenuOverlay.classList.add("hidden");
                }
            }, 300);
        }

        function toggleMegaMenu() {
            if (!megaMenu || !megaMenuOverlay) return;
            
            // Ensure mobile menu is closed
            if (isMobileMenuOpen) closeMobileMenu();

            if (isMegaMenuOpen) {
                closeMegaMenu();
            } else {
                isMegaMenuOpen = true;
                megaMenu.classList.remove("hidden");
                megaMenuOverlay.classList.remove("hidden");
                // Force reflow
                void megaMenu.offsetWidth;
                megaMenu.classList.remove("opacity-0");
                megaMenuOverlay.classList.remove("opacity-0");
                megaMenu.classList.add("opacity-100");
                megaMenuOverlay.classList.add("opacity-100");
                
                updateBurgerIcon(true);
                
                // Disable scroll
                const scrollbarWidth = window.innerWidth - document.documentElement.clientWidth;
                document.body.style.paddingRight = `${scrollbarWidth}px`;
                document.body.style.overflow = "hidden";
                if (nav) {
                    nav.style.paddingRight = `${scrollbarWidth}px`;
                }
                if (megaMenu) {
                    megaMenu.style.width = 'auto';
                    megaMenu.style.right = `${scrollbarWidth}px`;
                }
            }
        }

        function handleToggleClick() {
            if (window.innerWidth < 1024) {
                toggleMobileMenu();
            } else {
                toggleMegaMenu();
            }
        }

        if (megaMenuToggle) {
            megaMenuToggle.addEventListener("click", handleToggleClick);
        }

        if (megaMenuOverlay) {
            megaMenuOverlay.addEventListener("click", closeMegaMenu);
        }

        // Close menu when clicking on links
        const mobileMenuLinks = document.querySelectorAll("#mobileMenu a");
        mobileMenuLinks.forEach((link) => {
            link.addEventListener("click", closeMobileMenu);
        });

    })();
</script>

